{
  "name": "narrowland",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "vitest run",
    "test:ci": "tsc -p tsconfig.test.json && pnpm run test",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ci": "tsc -p tsconfig.test.json && pnpm run test:coverage",
    "typecheck": "tsc --noEmit",
    "knip": "knip",
    "size": "size-limit"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@rslib/core": "^0.15.0",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@types/node": "^22.18.9",
    "@vitest/coverage-v8": "^3.2.4",
    "expect-type": "^1.2.2",
    "husky": "^9.1.7",
    "knip": "^5.64.3",
    "lint-staged": "^16.2.4",
    "preset-small-lib": "link:@size/limit/preset-small-lib",
    "size-limit": "^11.2.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "private": true
}
