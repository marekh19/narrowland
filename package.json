{
  "name": "narrowland",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "vitest run",
    "test:ci": "tsc -p tsconfig.test.json && pnpm run test",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@rslib/core": "^0.15.0",
    "@types/node": "^22.18.6",
    "expect-type": "^1.2.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "private": true
}
